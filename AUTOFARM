-- Scripts:

local function TZOF_fake_script() -- Inject.LocalScript 
	local script = Instance.new('LocalScript', Inject)

	function onclick()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Varomine/SOME-HUB-MY/main/Bloxfruits%20open%20src.txt", true))()
	end
	
	script.Parent.MouseButton1Click:connect(onclick)
end
coroutine.wrap(TZOF_fake_script)()
local function VQTQS_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(VQTQS_fake_script)()
